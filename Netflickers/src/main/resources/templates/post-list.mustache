  {{>layout/header}}
    <h1>게시판목록</h1>
    <table class="table table-horizontal table-bordered">
    	<thead class="thead-strong">
    	<tr>
    		<th>No.</th>
    		<th>제목</th>   
    		<th>작성일자</th>    
    		<th>조회수</th>    				 		
    	</tr>
    	</thead>
    	<tbody>
    	{{#postings}}
    	<tr>
	    	<td>{{id}}</td>
	    	<td><a href="/post/read/{{id}}">{{title}}</a></td>
	    	<td>{{createdDate}}</td>
	    	<td>{{views}}</td>
    	</tr>
    	{{/postings}}
    	</tbody>
    </table>
 {{>layout/footer}}