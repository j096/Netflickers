 {{>layout/header}}
 <main class="form-signin w-100 m-auto">
	 <form>
	 	<div class="text-center">
		 <h1 class="h3 mb-3 fw-normal">로그인</h1>
		 </div>
		 <div class="form-floating">
		 	<input type="id" class="form-control" id="loginId" name="loginId" placeholder="Id">
		 	<label for="loginId">Id</label>
		 </div>
		 <div class="form-floating">
		  	<input type="password" class="form-control" id="password" name="loginId" placeholder="Password">
		 	<label for="password">Password</label>	
		 </div>
		 <button class="w-100 mb-1 btn btn-lg btn-primary" type="submit">로그인</button>
    	 <button class="w-100 mb-1 btn btn-lg btn-primary" type="button" onClick="goTo('/signup')">회원가입</button>
		 
	 </form>
 </main>
 {{>layout/footer}}
 